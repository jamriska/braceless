// Header with traditional include guard
#ifndef GUARDED_BLH
#define GUARDED_BLH

class GuardedClass:
private:
    int value

public:
    GuardedClass() : value(0) {}
    GuardedClass(int v) : value(v) {}

    int getValue() const:
        return value

    void setValue(int v):
        value = v

#endif // GUARDED_BLH
